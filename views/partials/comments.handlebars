<div class="comments">
  {{#each this}}
  <section class="comment">
    <div class="meta">
      {{user.username}} on {{format_date created_at}}
    </div>
    <div class="text">
      {{comment_text}} <button type="button" class="upvote-btn">Task Complete!</button>
    </div>
  </section>
  {{/each}}
</div>