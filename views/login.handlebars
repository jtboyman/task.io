<!--ADMIN LOGIN -->

<h1 class="sign-form-title">Sign Up or Log In here</h1>
<form class="login-form" id="admin-login-form">
  <div>
    <p>Admin Login</p>
  </div>
  <div>
    <label for="admin-email-login">email:</label>
    <input type="text" id="admin-email-login" />
  </div>
  <div>
    <label for="admin-password-login">password:</label>
    <input type="password" id="admin-password-login" />
  </div>
  <div>
    <button type="submit">login</button>
  </div>
</form>
<!--USER LOGIN -->
<form class="login-form" id="user-login-form">
